#: _backports.py:57
msgid "The two password fields didn’t match."
msgstr "Hasła nie są identyczne."

#: _backports.py:61
msgid "Password"
msgstr "Hasło"

#: _backports.py:61
msgid "Password confirmation"
msgstr "Potwierdzenie hasła"

#: _backports.py:74
msgid "Enter the same password as before, for verification."
msgstr "Wprowadź to samo hasło co wcześniej, aby je zweryfikować."

#: backends/activation/forms.py:26
msgid "This account has expired."
msgstr "To konto wygasło."

#: backends/activation/forms.py:27
msgid "The activation key you provided is invalid."
msgstr "Podany klucz aktywacyjny jest nieprawidłowy."

#: backends/activation/forms.py:29
msgid "Activation key"
msgstr "Klucz aktywacyjny"

#: backends/activation/views.py:171
msgid "The account you tried to activate has already been activated."
msgstr "Konto, które próbowałeś/aś aktywować, zostało już aktywowane."

#: backends/activation/views.py:173
msgid "The account you attempted to activate is invalid."
msgstr "Konto, które próbowałeś/aś aktywować, jest nieprawidłowe."

#: forms.py:335
msgid "I have read and agree to the Terms of Service"
msgstr "Przeczytałem/am i zgadzam się z Warunkami korzystania z usługi."

#: validators.py:19
msgid "This name cannot be registered. Please choose a different name."
msgstr "Ta nazwa użytkownika nie może zostać wybrana. Wybierz inną."

#: validators.py:21
msgid ""
"This email address cannot be registered. Please supply a different email "
"address."
msgstr "Ten adres email nie może zostać wybrany. Użyj innego adresu email."

#: validators.py:24
msgid ""
"This email address is already in use. Please supply a different email "
"address."
msgstr "Ten adres email jest już używany. Użyj innego adresu email."

#: validators.py:26
msgid "A user with that username already exists."
msgstr "Użytkownik o tej nazwie już istnieje."

#: validators.py:28
msgid ""
"Registration using free email addresses is prohibited. Please supply a "
"different email address."
msgstr ""
"Nie ma możliwości rejestracji przy użyciu darmowego adresu email. Użyj "
"innego adresu email."

#: validators.py:31
msgid "This name is reserved and cannot be registered."
msgstr "Ta nazwa jest zarezerwowana i nie może zostać zarejestrowana."

#: validators.py:32
msgid "You must agree to the terms to register"
msgstr "Musisz zaakceptować regulamin, aby się zarejestrować"
